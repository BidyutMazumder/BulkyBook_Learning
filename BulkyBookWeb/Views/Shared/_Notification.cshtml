@if(TempData["success"] != null){
    <script type="text/javascript">
        toastr.success('@TempData["success"]',{ timeOut: 1000 })
    </script>
}
else if (TempData["error"] != null)
{
    <script type="text/javascript">
        toastr.error('@TempData["error"]',{ timeOut: 1000 })
    </script>
}